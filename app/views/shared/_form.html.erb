<div class="container">
  <div class="row">

      <%= form_for @plant do |f| %>
    <%= f.label :name, "Name" %>
    <%= f.text_field :name %>
    <%= f.label :description, "description" %>
    <%= f.text_field :description %>
    <%= f.label :location, "location" %>
    <%= f.text_field :location %>
    <%= f.label :price_per_day, "Price Per Day" %>
    <%= f.text_field :price_per_day %>
    <%= f.submit %>
  </div>

  <% end %>
</div>
